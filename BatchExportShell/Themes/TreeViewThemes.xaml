<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style TargetType="TreeViewItem" x:Key="TreeViewCard">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="TreeViewItem">
                    <Border Background="{TemplateBinding Background}" Height="50" CornerRadius="4" Margin="3">
                        <StackPanel Orientation="Horizontal" Margin="10,0,0,0" Background="#182B49">
                            <Border Width="30" Height="30" CornerRadius="25"/>
                            <!--<TextBlock Text="{Binding}"-->
                        </StackPanel>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>

    </Style>


</ResourceDictionary>